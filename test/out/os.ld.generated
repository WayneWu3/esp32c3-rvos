ENTRY(_start)
MEMORY
{
 irom (rwxa) : ORIGIN = 0x42000000, LENGTH = 4*1024*1024
}
SECTIONS
{
     . = 0x42000000;
   .header :{
             LONG(0xaedb041d);
      LONG(0xaedb041d);
   }>irom
 .text :{
      PROVIDE(_text_start = .);
      *(.text .text.*)
      PROVIDE(_text_end = .);
 }>irom
 .rodata :{
      PROVIDE(_rodata_start = .);
      *(.rodata .rodata.*)
      PROVIDE(_rodata_end = .);
 }>irom
 .data :{
      . = ALIGN(4096);
      PROVIDE(_data_start = .);
      *(.sdata .sdata.*)
      *(.data .data.*)
      PROVIDE(_data_end = .);
 }>irom
 .bss :{
      PROVIDE(_bss_start = .);
      *(.sbss .sbss.*)
      *(.bss .bss.*)
      *(COMMON)
      PROVIDE(_bss_end = .);
 }>irom
 PROVIDE(_memory_start = ORIGIN(irom));
 PROVIDE(_memory_end = ORIGIN(irom) + LENGTH(irom));
 PROVIDE(_heap_start = _bss_end);
 PROVIDE(_heap_size = _memory_end - _heap_start);
}
PROVIDE(printf = 0x40000040);
